{
  "name": "facebookをメイリオで",
  "version": "0.1",
  "permissions": ["geolocation"],
  "manifest_version": 2,
  
  "content_scripts": [
    {
      "matches": [
	"http://*.facebook.com/*",
	"https://*.facebook.com/*"
	],
      "js": [
        "jquery.min.1.8.2.js",
        "font-change.js"
	]
    }
  ]

}